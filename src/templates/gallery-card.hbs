{{#each this}}
<li class="photo-card list">
    <a href="{{this.largeImageURL}}" class="link">
        <img src="{{this.webformatURL}}" alt="{{this.tags}}" loading="lazy" />
    </a>
    <div class="info">
        <p class="info-text">
            <b>Likes</b>{{this.likes}}
        </p>
        <p class="info-text">
            <b>Views</b>{{this.views}}
        </p>
        <p class="info-text">
            <b>Comments</b>{{this.comments}}
        </p>
        <p class="info-text">
            <b>Downloads</b>{{this.downloads}}
        </p>
    </div>
</li>
{{/each}}